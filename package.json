{
  "name": "vanilla-bean",
  "version": "1.0.0",
  "description": "A webpack-based starter for Vanilla JS Single Page Applications.",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js  --colors",
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "pretty-quick": "pretty-quick --staged --pattern \"src/**/*.js\"",
    "prettier": "prettier --write src/**/*.js",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbsonley/vanilla-bean.git"
  },
  "keywords": [
    "webpack",
    "page.js",
    "frontend",
    "es6",
    "javascript",
    "hogan"
  ],
  "author": "Ben Sonley <lbsonley@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lbsonley/vanilla-bean/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "raw-loader": "^4.0.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "core-js": "^3.4.2",
    "hogan.js": "^3.0.2",
    "normalize.css": "^8.0.1",
    "page": "^1.11.5"
  }
}
